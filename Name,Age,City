#include <stdio.h>
int main() {
    char name[20], city[20];
    int age;

    printf("Enter name: ");
    scanf("%s", name);

    printf("Enter age: ");
    scanf("%d", &age);

    printf("Enter city: ");
    scanf("%s", city);

    printf("Name: %s\nAge: %d\nCity: %s", name, age, city);
    return 0;
}
